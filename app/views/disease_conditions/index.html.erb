<h1>Listing disease_conditions</h1>

<table>
  <thead>
    <tr>
      <th>Icd 10</th>
      <th>Patient visit</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @disease_conditions.each do |disease_condition| %>
      <tr>
        <td><%= disease_condition.icd_10 %></td>
        <td><%= disease_condition.patient_visit %></td>
        <td><%= link_to 'Show', disease_condition %></td>
        <td><%= link_to 'Edit', edit_disease_condition_path(disease_condition) %></td>
        <td><%= link_to 'Destroy', disease_condition, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Disease condition', new_disease_condition_path %>
